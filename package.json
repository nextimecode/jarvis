{
  "name": "jarvis",
  "version": "0.0.0-development",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "clean": "rm -rf node_modules .next yarn.lock package-lock.json && yarn cache clean",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --ext .tsx,.ts",
    "lint:fix": "eslint src --ext .tsx,.ts --fix",
    "precommit": "yarn && yarn lint && yarn audit",
    "commit": "cz",
    "prepush": "yarn precommit",
    "go": "yarn prepush && yarn dev",
    "prepare": "husky install",
    "postinstall": "husky install",
    "codegen": "graphql-codegen",
    "test": "jest",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@apollo/client": "latest",
    "@chakra-ui/icons": "latest",
    "@chakra-ui/react": "latest",
    "@emotion/react": "latest",
    "@emotion/styled": "latest",
    "@fontsource/open-sans": "latest",
    "@fontsource/rajdhani": "latest",
    "eslint-plugin-n": "latest",
    "framer-motion": "latest",
    "graphql": "latest",
    "next": "latest",
    "prettier": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-icons": "latest"
  },
  "devDependencies": {
    "@babel/core": "latest",
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@graphql-codegen/cli": "latest",
    "@graphql-codegen/typescript": "latest",
    "@graphql-codegen/typescript-operations": "latest",
    "@graphql-codegen/typescript-react-apollo": "latest",
    "@testing-library/jest-dom": "latest",
    "@testing-library/react": "latest",
    "@types/node": "latest",
    "@types/react": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "babel-plugin-inline-react-svg": "latest",
    "commitizen": "latest",
    "cz-conventional-changelog": "latest",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "eslint-config-prettier": "latest",
    "eslint-config-standard": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "eslint-plugin-standard": "latest",
    "husky": "latest",
    "jest": "latest",
    "jest-environment-jsdom": "latest",
    "lint-staged": "latest",
    "semantic-release": "latest",
    "typescript": "latest"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nextimecode/jarvis.git"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
